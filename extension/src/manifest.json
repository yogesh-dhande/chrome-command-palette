{
  "name": "Single Dispatch",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Get a command palette on every website. Navigate through any web page, tabs, or bookmarks with your keyboard.",
  "action": {
    "default_title": "Single Dispatch Command Palette"
  },
  "icons": {
    "16": "/assets/16x16.png",
    "48": "/assets/48x48.png",
    "128": "/assets/128x128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/main.js"]
    }
  ],
  "commands": {
    "activate_extension": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "Activate Single Dispatch Command Palette"
    }
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://singledispatch.web.app/*",
      "https://singledispatch.com/*",
      "https://www.singledispatch.com/*"
    ]
  },
  "permissions": ["tabs"]
}
