{
  "*": [
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "a"
        },
        "label": {
          "template": "{innerText}"
        },
        "trigger": {
          "type": "open"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "button"
        },
        "label": {
          "template": "button"
        },
        "trigger": {
          "type": "click"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "input"
        },
        "label": {
          "template": "input"
        },
        "trigger": {
          "type": "focus"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[type=checkbox]"
        },
        "label": {
          "template": "input"
        },
        "trigger": {
          "type": "click"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[type=radio]"
        },
        "label": {
          "template": "input"
        },
        "trigger": {
          "type": "click"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[type=file]"
        },
        "label": {
          "template": "input"
        },
        "trigger": {
          "type": "click"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "select"
        },
        "label": {
          "template": "input"
        },
        "trigger": {
          "type": "focus"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "textarea"
        },
        "label": {
          "template": "input"
        },
        "trigger": {
          "type": "focus"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[role=link]"
        },
        "label": {
          "template": "{innerText}"
        },
        "trigger": {
          "type": "open"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[role=button]"
        },
        "label": {
          "template": "button"
        },
        "trigger": {
          "type": "click"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[role=checkbox]"
        },
        "label": {
          "template": "button"
        },
        "trigger": {
          "type": "click"
        }
      }
    }
  ],
  "github.com": [
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[data-hydro-click]"
        },
        "label": {
          "template": "button"
        },
        "trigger": {
          "type": "click"
        }
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/pulls",
        "label": "Pull Requests: Created"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/pulls/assigned",
        "label": "Pull Requests: Assigned"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/pulls/mentioned",
        "label": "Pull Requests: Mentioned"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/pulls/review-requested",
        "label": "Pull Requests: Review Requests"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/issues",
        "label": "Issues: Created"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/issues/assigned",
        "label": "Issues: Assigned"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "/issues/mentioned",
        "label": "Issues: Mentioned"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "https://github.com/new",
        "label": "Create a new repo"
      }
    },
    {
      "type": "link",
      "link": {
        "url": "https://gist.github.com",
        "label": "Create a new gist"
      }
    },
    {
      "type": "element",
      "element": {
        "scope": "#branch-select-menu summary",
        "label": {
          "template": "Switch branches"
        },
        "trigger": {
          "type": "click"
        },
        "sequence": [
          {
            "type": "element",
            "element": {
              "scope": null,
              "trigger": {
                "selector": "#branch-select-menu .SelectMenu-tab:nth-child(1)",
                "type": "click"
              }
            }
          }
        ]
      }
    },
    {
      "type": "element",
      "element": {
        "scope": "#branch-select-menu summary",
        "label": {
          "template": "Switch tags"
        },
        "trigger": {
          "type": "click"
        },
        "next": [
          {
            "type": "element",
            "element": {
              "scope": null,
              "trigger": {
                "selector": "#branch-select-menu .SelectMenu-tab:nth-child(2)",
                "type": "click"
              }
            }
          }
        ]
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "get-repo summary[data-hydro-click]"
        },
        "label": {
          "template": "Clone the repo"
        },
        "trigger": {
          "type": "click"
        },
        "next": {
          "type": "element",
          "element": {
            "label": {
              "template": "Copy URL"
            },
            "trigger": {
              "selector": "clipboard-copy[value*='http']",
              "type": "click"
            }
          }
        }
      }
    }
  ],
  "mail.google.com": [
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": ".aeH [title]"
        },
        "label": {
          "template": "button"
        },
        "trigger": {
          "type": "simulatedClick"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": ".aTN"
        },
        "label": {
          "template": "Add/remove star"
        },
        "trigger": {
          "selector": "[aria-label*=\"tarred\"]",
          "type": "click"
        }
      }
    },
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "[role=\"row\"]"
        },
        "label": {
          "selector": "[role=\"link\"]",
          "template": "{innerText}"
        },
        "trigger": {
          "selector": "[role=\"link\"]",
          "type": "open"
        },
        "options": [
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Archive"
              },
              "trigger": {
                "selector": "[data-tooltip=\"Archive\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Delete"
              },
              "trigger": {
                "selector": "[data-tooltip=\"Delete\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Mark Unread"
              },
              "trigger": {
                "selector": "[data-tooltip=\"Mark as unread\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Mark Read"
              },
              "trigger": {
                "selector": "[data-tooltip=\"Mark as read\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Snooze"
              },
              "trigger": {
                "selector": "[data-tooltip=\"Snooze\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Star"
              },
              "trigger": {
                "selector": "[role=\"button\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Select"
              },
              "trigger": {
                "selector": "[role=\"checkbox\"]",
                "type": "click"
              },
              "allowHidden": true
            }
          }
        ]
      }
    }
  ],
  "twitter.com": [
    {
      "type": "element",
      "element": {
        "scope": {
          "selector": "article"
        },
        "label": {
          "selector": "[data-testid=\"tweetText\"]",
          "template": "{innerText}"
        },
        "trigger": {
          "type": "click"
        },
        "options": [
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Reply"
              },
              "trigger": {
                "selector": "[data-testid=\"reply\"]",
                "type": "click"
              }
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Retweet"
              },
              "trigger": {
                "selector": "[data-testid=\"retweet\"]",
                "type": "click"
              }
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Like"
              },
              "trigger": {
                "selector": "[data-testid=\"like\"]",
                "type": "click"
              }
            }
          },
          {
            "type": "element",
            "element": {
              "label": {
                "template": "Share"
              },
              "trigger": {
                "selector": "[aria-label=\"Share Tweet\"]",
                "type": "click"
              }
            }
          }
        ]
      }
    }
  ]
}
